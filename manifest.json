{
  "name": "SanjuRegex",
  "version": "2.0",
  "manifest_version": 2,
  "description": "Word Replacer",
  "permissions": ["storage", "declarativeContent"],
  "icons": {
      "16": "jedilogo16.png",
      "32": "jedilogo32.png",
      "48": "jedilogo48.png",
      "128": "jedilogo128.png"
    },
  "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
